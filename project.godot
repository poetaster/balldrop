[application]

config/name="balldrop"
run/main_scene="res://main.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="harbour-balldrop"
config/icon="res://icon.png"

[autoload]

AudioPlayer="*res://global_audioplayer.gd"
InputManager="*res://plugins/GodotTouchInputManager/InputManager.gd"

[display]

window/size/width=144
window/size/height=256
window/size/test_width=288
window/size/test_height=512
window/handheld/orientation="portrait"

[global]

name=false

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

quality/driver/driver_name="GLES2"
quality/driver/fallback_to_gles2=true
quality/depth_prepass/enable=false
environment/default_environment="res://default_env.tres"
